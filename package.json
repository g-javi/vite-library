{
  "name": "react-library",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --p ./tsconfig.lib.json && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prettier": "prettier --check \"**/*.{ts,tsx,scss}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx,scss}\"",
    "vitest": "vitest run",
    "vitest:watch": "vitest"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./esm/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./cjs/index.cjs"
      }
    },
    "./pages": {
      "import": {
        "types": "./lib/pages/index.d.ts",
        "default": "./esm/pages/index.mjs"
      },
      "require": {
        "types": "./lib/pages/index.d.ts",
        "default": "./cjs/pages/index.cjs"
      }
    },
    "./components": {
      "import": {
        "types": "./lib/components/index.d.ts",
        "default": "./esm/components/index.mjs"
      },
      "require": {
        "types": "./lib/components/index.d.ts",
        "default": "./cjs/components/index.cjs"
      }
    }
  },
  "dependencies": {
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@storybook/addon-a11y": "9.1.7",
    "@storybook/addon-docs": "9.1.7",
    "@storybook/addon-onboarding": "9.1.7",
    "@storybook/addon-vitest": "9.1.7",
    "@storybook/react-vite": "9.1.7",
    "@types/node": "24.5.2",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react-swc": "4.1.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.36.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "eslint-plugin-storybook": "9.1.7",
    "globals": "16.4.0",
    "playwright": "1.55.0",
    "prettier": "3.6.2",
    "sass-embedded": "1.93.0",
    "storybook": "9.1.7",
    "typescript": "~5.8.3",
    "typescript-eslint": "8.44.0",
    "vite": "7.1.7",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-lib-inject-css": "2.2.2",
    "vitest": "3.2.4"
  }
}
